#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int num1;

	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	printf("Ingresa un numero: ");
	scanf("%d",&num1);
	printf("ingresate el num %d",num1);



	return EXIT_SUCCESS;
}
